#include<bits/stdc++.h>
using namespace std;
class solution 
{
  public:
     int sum(int a,int b)
     {
         if(b==0)  return a;
          int summ =a^b; //add witgout carrying
          int carry =(a&b)<<1;  // carry , but don't add
          return sum(summ,carry); // recurse sum + carry
     }
};

 int main()
 {
     int t;
      cin>>t;
      while(t--)
      {
          int a,b;
          cin>>a>>b;
          solution obj;
          cout<<obj.sum(a,b)<<endl;
      }
       return 0;
 }
